<?xml version="1.0" encoding="UTF-8"?>
<!-- Configuration of the Ant build system to generate a Jar file --> 
<project name="qparser" default="create_qparser_jar">
  <target name="create_qparser_jar" description="Create Jar file for qparser">
		<mkdir dir="bin/"/>
		<jar jarfile="bin/qparser.jar" basedir="bin/" includes="qparser/*.class" />
  </target>
</project>
